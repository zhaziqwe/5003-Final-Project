{
  "lightgbm": {
    "num_leaves": {
      "type": "int",
      "low": 20,
      "high": 150,
      "log": false
    },
    "max_depth": {
      "type": "int",
      "low": 3,
      "high": 12,
      "log": false
    },
    "learning_rate": {
      "type": "float",
      "low": 0.01,
      "high": 0.3,
      "log": true
    },
    "n_estimators": {
      "type": "int",
      "low": 100,
      "high": 1000,
      "log": false
    },
    "min_child_samples": {
      "type": "int",
      "low": 10,
      "high": 100,
      "log": false
    },
    "subsample": {
      "type": "float",
      "low": 0.6,
      "high": 1.0,
      "log": false
    },
    "colsample_bytree": {
      "type": "float",
      "low": 0.6,
      "high": 1.0,
      "log": false
    },
    "reg_alpha": {
      "type": "float",
      "low": 0.0,
      "high": 10.0,
      "log": false
    },
    "reg_lambda": {
      "type": "float",
      "low": 0.0,
      "high": 10.0,
      "log": false
    }
  },
  "svm": {
    "C": {
      "type": "float",
      "low": 0.001,
      "high": 100.0,
      "log": true
    },
    "kernel": {
      "type": "categorical",
      "choices": ["rbf", "linear", "poly"]
    },
    "gamma": {
      "type": "float",
      "low": 0.0001,
      "high": 10.0,
      "log": true
    },
    "degree": {
      "type": "int",
      "low": 2,
      "high": 5,
      "log": false
    },
    "class_weight": {
      "type": "categorical",
      "choices": ["balanced", null]
    }
  },
  "mlp": {
    "hidden_layer_sizes": {
      "type": "categorical",
      "choices": [
        [128],
        [256],
        [512],
        [128, 64],
        [256, 128],
        [512, 256],
        [256, 128, 64],
        [512, 256, 128]
      ]
    },
    "learning_rate_init": {
      "type": "float",
      "low": 0.0001,
      "high": 0.01,
      "log": true
    },
    "alpha": {
      "type": "float",
      "low": 0.0001,
      "high": 0.1,
      "log": true
    },
    "batch_size": {
      "type": "categorical",
      "choices": [32, 64, 128, 256]
    },
    "activation": {
      "type": "categorical",
      "choices": ["relu", "tanh"]
    },
    "max_iter": {
      "type": "int",
      "low": 200,
      "high": 1000,
      "log": false
    },
    "early_stopping": {
      "type": "categorical",
      "choices": [true, false]
    }
  }
}

